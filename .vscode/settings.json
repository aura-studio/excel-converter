{
    "editor.defaultFormatter": null,
    "editor.tabSize": 4,
    "editor.formatOnType": true,
    "editor.formatOnSaveMode": "file",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.parameterHints.enabled": true,
    "editor.tabCompletion": "on",
    "editor.insertSpaces": false,
    "editor.fontFamily": "Cascadia Code",
    "terminal.integrated.fontFamily": "Cascadia Code",
    "extensions.ignoreRecommendations": false,
    "search.exclude": {
        "vendor/*": true,
    },
    "git.autofetchPeriod": 60,
    "explorer.decorations.colors": true,
    "explorer.decorations.badges": true,
    "problems.decorations.enabled": true,
    "workbench.colorCustomizations": {
        "problemsErrorIcon.foreground": "#DA6A63",
        "list.errorForeground": "#DA6A63",
        "explorer.decorations.errorBadgeBackground": "#DA6A63",
        "explorer.decorations.errorBadgeForeground": "#ffffff",
        "editorError.foreground": "#DA6A63",
        "scrollbarSlider.activeBackground": "#DA6A63",
        "editorOverviewRuler.errorForeground": "#DA6A63"
    },
    "git.autofetch": true,
    "git.fetchOnPull": true,
    "git.autorefresh": true,
    "git.ignoreLimitWarning": true,
    "files.eol": "\n",
    "files.encoding": "utf8",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 5000,
    "files.exclude": {
        "**/.git": true,
    },
    "files.watcherExclude": {
        "**/.git": true,
    },
    "go.testTimeout": "3600s",
    "go.testFlags": [
        "-v",
        "-count=1"
    ],
    "go.testEnvVars": {
        "GO111MODULE": "on"
    },
    "go.formatTool": "gofumpt",
    "go.formatFlags": [],
    "go.lintTool": "golangci-lint",
    "go.lintOnSave": "package",
    "go.lintFlags": [
        "--fast",
        "--fix"
    ],
    "go.vetOnSave": "package",
    "go.vetFlags": [
        "-all"
    ],
    "go.enableCodeLens": {
        "runtest": true
    },
    "go.useLanguageServer": true,
    "go.languageServerExperimentalFeatures": {
        "diagnostics": true,
        "documentLink": true
    },
    "go.gotoSymbol.ignoreFolders": [],
    "go.gotoSymbol.includeGoroot": true,
    "go.gotoSymbol.includeImports": true,
    "go.autocompleteUnimportedPackages": true,
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
    "go.liveErrors": {
        "enabled": true,
        "delay": 500
    },
    "go.buildOnSave": "package",
    "go.inferGopath": false,
    "go.installDependenciesWhenBuilding": true,
    "go.coverOnTestPackage": true,
    "go.toolsManagement.checkForUpdates": "proxy",
    "go.toolsEnvVars": {
        "GOOS": "windows",
        "GOARCH": "amd64",
        "CGO_ENABLED": "1",
        "GO111MODULE": "auto"
    },
}